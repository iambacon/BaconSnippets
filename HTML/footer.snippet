<CodeSnippet Format="1.0.0"
             xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <Header>
        <Title>Footer</Title>
        <Author>Colin Bacon</Author>
        <Shortcut>footer</Shortcut>
        <Description>Html snippet to generate the HTML5 footer tag.</Description>
        <SnippetTypes>
            <SnippetType>Expansion</SnippetType>
        </SnippetTypes>
    </Header>
    <Snippet>
        <Declarations>
            <Literal>
                <ID>selected</ID>
                <ToolTip>content</ToolTip>
                <Default>content</Default>
            </Literal>
        </Declarations>
        <Code Language="html">
            <![CDATA[ <footer>$selected$</footer>$end$]]>
        </Code>
    </Snippet>
</CodeSnippet>