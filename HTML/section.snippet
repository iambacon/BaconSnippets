<CodeSnippet Format="1.0.0"
             xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>Section</Title>
    <Author>Colin Bacon</Author>
    <Shortcut>section</Shortcut>
    <Description>Html snippet to generate the HTML5 section tag.</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>selected</ID>
        <ToolTip>content</ToolTip>
        <Default>content</Default>
      </Literal>
    </Declarations>
    <Code Language="html">
      <![CDATA[ <section>$selected$</section>$end$]]>
    </Code>
  </Snippet>
</CodeSnippet>